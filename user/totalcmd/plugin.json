{
  "plugin": {
    "name": "totalcmd",
    "author": "Array",
    "version": 1,
    "info": "totalcmd 插件",
    "include": "totalcmd.ahk",
    "init": "totalcmd_init"
  },
  "vimd": {
    "name": "totalcmd",
    "winclass": "TTOTAL_CMD",
    "winexe": "totalcmd.exe",
    "onBeforekey": "totalcmd_onbeforekey",
    "maxCount": 999,
    "timeout": 500,
    "mode": {
      "normal": {
        "mapnum": true,
        "default": true,
        "map": {
          "<esc>": "totalcmd_esc",
          "j": "down",
          "k": "up",
          "l": "right",
          "h": "left",
          "<s-h>": "cm_GotoPreviousDir",
          "<s-l>": "cm_GotoNextDir",
          "d": "cm_DirectoryHotlist",
          "<s-d>": "cm_OpenDesktop",
          "i": "totalcmd_change_to_insert",
          "e": "cm_ContextMenu",
          "<s-e>": "cm_ExecuteDOS",
          "<s-n>": "cm_DirectoryHistory",
          "o": "cm_LeftOpenDrives",
          "<s-o>": "cm_RightOpenDrives",
          "q": "cm_SrcQuickview",
          "r": "cm_RenameOnly",
          "u": "cm_GoToParent",
          "<s-r>": "cm_MultiRenameFiles",
          "x": "cm_Delete",
          "w": "cm_List",
          "y": "cm_CopyNamesToClip",
          "<s-y>": "cm_CopyFullNamesToClip",
          "p": "cm_UnpackFiles",
          "<s-p>": "cm_packFiles",
          "t": "cm_OpenNewTab",
          "<s-t>": "cm_OpenNewTabBg",
          "<s-u>": "cm_GoToRoot",
          "n": "totalcmd_azhistory",
          "/": "cm_ShowQuickSearch",
          ".": "repeat",
          "?": "cm_SearchFor",
          "[": "cm_SelectCurrentName",
          "]": "cm_SelectCurrentExtension",
          "{": "cm_UnSelectCurrentName",
          "}": "cm_UnselectCurrentExtension",
          "-": "cm_SwitchSeparateTree",
          "\\": "cm_ExchangeSelection",
          "\\|": "cm_ClearAll",
          ":": "cm_FocusCmdLine",
          "=": "cm_MatchSrc",
          ";": "cm_FocusCmdLine",
          "cc": "cm_Copy",
          "co": "cm_CopyOtherpanel",
          "cs": "cm_CopySamepanel",
          "cb": "cm_CopyToClipboard",
          "ct": "cm_CutToClipboard",
          "cv": "cm_PasteFromClipboard",
          "ga": "cm_CloseAllTabs",
          "gn": "cm_SwitchToNextTab",
          "gp": "cm_SwitchToPreviousTab",
          "gc": "cm_CloseCurrentTab",
          "gb": "cm_OpenDirInNewTabOther",
          "ge": "cm_Exchange",
          "gw": "cm_ExchangeWithTabs",
          "g1": "cm_SrcActivateTab1",
          "g2": "cm_SrcActivateTab2",
          "g3": "cm_SrcActivateTab3",
          "g4": "cm_SrcActivateTab4",
          "g5": "cm_SrcActivateTab5",
          "g6": "cm_SrcActivateTab6",
          "g7": "cm_SrcActivateTab7",
          "g8": "cm_SrcActivateTab8",
          "g9": "cm_SrcActivateTab9",
          "sn": "cm_SrcByName",
          "se": "cm_SrcByExt",
          "ss": "cm_SrcBySize",
          "sd": "cm_SrcByDateTime",
          "sr": "cm_SrcNegOrder",
          "s1": "cm_SrcSortByCol1",
          "s2": "cm_SrcSortByCol2",
          "s3": "cm_SrcSortByCol3",
          "s4": "cm_SrcSortByCol4",
          "s5": "cm_SrcSortByCol5",
          "s6": "cm_SrcSortByCol6",
          "s7": "cm_SrcSortByCol7",
          "s8": "cm_SrcSortByCol8",
          "s9": "cm_SrcSortByCol9",
          "s0": "cm_SrcUnsorted",
          "v": "cm_SrcCustomViewMenu",
          "Vb": "cm_VisButtonbar",
          "Vd": "cm_VisDriveButtons",
          "Vo": "cm_VisTwoDriveButtons",
          "Vr": "cm_VisDriveCombo",
          "Vc": "cm_VisDriveCombo",
          "Vt": "cm_VisTabHeader",
          "Vs": "cm_VisStatusbar",
          "Vn": "cm_VisCmdLine",
          "Vf": "cm_VisKeyButtons",
          "Vw": "cm_VisDirTabs",
          "Ve": "cm_CommandBrowser",
          "zz": "cm_50Percent",
          "zn": "cm_Minimize",
          "zm": "cm_Maximize",
          "zr": "cm_Restore",
          "zv": "cm_VerticalPanels"
        }
      },
      "insert": {
        "map": {
          "<esc>": "totalcmd_change_to_normal"
        }
      }
    }
  }
}
